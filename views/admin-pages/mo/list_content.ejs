<div class="right_col" role="main">
  <div class="">

<div class="col-md-12 col-sm-12 ">
  <div class="x_panel">
    <div class="x_title">
      <h2>Math olympiad<small>Participent List</small></h2>
      <ul class="nav navbar-right panel_toolbox">
        <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
        </li>
        <li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
          <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
              <a class="dropdown-item" href="#">Settings 1</a>
              <a class="dropdown-item" href="#">Settings 2</a>
            </div>
        </li>
        <li><a class="close-link"><i class="fa fa-close"></i></a>
        </li>
      </ul>
      <div class="clearfix"></div>
    </div>
    <div class="x_content">
        <div class="row">
            <div class="col-sm-12">
              <div class="card-box table-responsive">

      <table id="datatable-responsive" class="table table-striped table-bordered dt-responsive nowrap" cellspacing="0" width="100%">
        <thead>
          <tr>
            <th>name</th>
            <th>Catagory</th>
            <th>Contact</th>
            <th>Email</th>
            <th>Institution</th>
            <th>Total fee</th>
            <th>paid</th>
            <th>Payment</th>
            <th>Selected</th>
            <th>T Shirt</th>
            <th>Date</th>
            <th>Select</th>
            <th>Payment Done</th>
            <th>Edit</th>
            <th>Delete</th>
          </tr>
        </thead>
        <tbody>
          <%if(perticipents.length>0){%>
            <%perticipents.forEach(perticipent =>{%>
              <tr>
                <td><%= perticipent.pname %></td>
                <td><%= perticipent.catagory %></td>
                <td><%= perticipent.contact %></td>
                <td><%= perticipent.email %></td>
                <td><%= perticipent.institution %></td>
                <td><%= perticipent.total %></td>
                <td><%= perticipent.paid %></td>
                <td><%if(perticipent.paid==perticipent.total){%>
                  <p style="color: green;">Successful</p> 
                  <%}else{%>
                  <p style="color: red;"> Incomplete</p>
                  <%}%> 
               </td>
               <td><%if(perticipent.selected==true){%>
                <p style="color: green;">Yes</p> 
                <%}else{%>
                <p style="color: red;">No</p>
                <%}%> 
              </td>
                <td><%= perticipent.t_shirt %></td>
                <td><%= perticipent.reg_date %></td>
                <td><%const selectUrl= "/select/" + perticipent.id %>
                  <a href="<%= selectUrl %>"
                  ><i class="fa fa-check"></i>
                  <span class="fa fa-chevron"></span>
                  </a>
                  </td>
                <td><%const paymentdoneUrl= "/paymentDone/" + perticipent.id %>
                  <a href="<%= paymentdoneUrl %>"
                  ><i class="fa fa-money"></i>
                  <span class="fa fa-chevron"></span>
                  </a>
                  </td>
                  <td><%const editUrl= "/edit/" + perticipent.id %>
                    <a href="<%= editUrl %>"
                    ><i class="fa fa-pencil"></i>
                    <span class="fa fa-chevron"></span>
                    </a>
                    </td>
                <td><%const deleteUrl= "/delete/" + perticipent.id %>
                <a href="<%= deleteUrl %>"
                ><i class="fa fa-trash"></i>
                <span class="fa fa-chevron"></span>
                </a>
                </td>
              </tr>
                <%})%>
         <%}%>
          
        </tbody>
      </table>


    </div>
  </div>
</div>
</div>
  </div>
</div>